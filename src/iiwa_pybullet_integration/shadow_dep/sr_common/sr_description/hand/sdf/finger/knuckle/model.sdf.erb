<?xml version='1.0'?>
<%
# This is where ruby code goes
M_PI = 3.14
#Mesh path for Ros

#Mesh path for pybullet
    pyb_forearm_lite_path_v = "model://knuckle/knuckle.dae"
#Current path
    forearm_lite_path_v = pyb_forearm_lite_path_v
%>
<sdf version='1.6'>
  <model name='knuckle'>
  <!-- Standard knuckle  -->
    <link name = "knuckle_link">
        <pose frame=''>0 0 0 0 0 0</pose>
        <inertial>
            <pose frame=''>0 0 0 0 0 0</pose>
            <mass>0.008</mass>
            <inertia>
                <ixx>0.00000026</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.00000026</iyy>
                <iyz>0</iyz>
                <izz>0.00000032</izz>
            </inertia>
        </inertial>
	  
        <visual name='visual'>
            
            <geometry>
                <mesh>
                    <scale>0.001 0.001 0.001</scale>
                    <uri><%= forearm_lite_path_v %></uri>
                </mesh>
            </geometry>
            <material>

            </material>
        </visual>
        <collision name='collision'>
            <geometry>
                <mesh>
                    <scale>0.001 0.001 0.001</scale>
                    <uri><%= forearm_lite_path_v %></uri>
                </mesh>
            </geometry>
        </collision>
    </link>
  </model>
</sdf>